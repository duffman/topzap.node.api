{
  "compilerOptions": {
  "module": "commonjs",
  "target": "es6",
  "noImplicitAny": false,
  "sourceMap": false,
  "experimentalDecorators": true,
  "emitDecoratorMetadata": true,
  "outDir": "dist",
  "baseUrl": "./",
  "paths": {
    "@core/*": ["./core/*"],
    "@miner/*": ["./miner/*"],
    "@models/*": ["./models/*"],
    "@db/*": ["./database/*"],
    "@utils/*": ["./utils/*"]
  }
},
  "exclude": [
    "node_modules"
  ]
}
